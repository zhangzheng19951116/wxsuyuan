.container{
  width: 99%;
  height: 200rpx;
  border:1px solid #FFA500;
   border-radius: 10rpx;
}
.input{
  width: 100%;
  height: 100rpx;
}
.maincontainer{
   display:flex;
   flex-direction:column;
   align-items:center;
}
.journey-container{
   display:flex;
   flex-direction:row;
    border: 1px solid #FF7A34;
    width: 400rpx;
    height: 100rpx;
    border-radius: 10px;
   justify-content:center;
    align-items: center;
    margin-top: 30rpx;
