.qrcode_dialog {
  width: 80%;
  margin: 0 10%;
  height: 560rpx;
  background: #fff;
  z-index: 999;
  position: fixed;
  bottom: 250rpx;
  top: 250rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  border-top: 1rpx #e2e2e2 solid;
  border-bottom: 1rpx #e2e2e2 solid;
  padding-bottom: 20rpx;
  -webkit-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
  box-shadow: 0 0 5rpx rgba(0, 0, 0, 0.3);
}

.qrcode_dialog .qrcode_bg{
  width: 60%;
  margin: 10% 20%;
  height: 360rpx;
}

.qrcode_dialog .qrcode_bg image{
  width: 100%;
  height: 360rpx;
}

.qrcode_dialog .btn_save{
  width: 60%;
  height: 70rpx;
  background: #0a96cb;
  margin: 0 20%;
  color: #fff;
  font-size: 26rpx;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}